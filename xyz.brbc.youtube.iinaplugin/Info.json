{
  "name": "YouTube",
  "identifier": "xyz.brbc.youtube",
  "version": "1.2.0",
  "ghRepo": "ada-bee/youtube-iina",
  "ghVersion": 3,
  "description": "Browse and play YouTube videos in IINA",
  "author": {
    "name": "ada-bee"
  },
  "entry": "dist/main.js",
  "globalEntry": "dist/global.js",
  "sidebarTab": {
    "name": "YouTube"
  },
  "preferencesPage": "ui/preferences.html",
  "preferenceDefaults": {
    "sponsorBlockEnabled": false,
    "sponsorSegmentAction": "ask",
    "selfPromoSegmentAction": "ignore",
    "previewSegmentAction": "ignore"
  },
  "permissions": [
    "network-request",
    "sidebar",
    "show-alert",
    "show-osd",
    "file-system",
    "video-overlay"
  ],
  "allowedDomains": [
    "youtube.com",
    "www.youtube.com",
    "i.ytimg.com",
    "yt3.ggpht.com",
    "sponsor.ajay.app"
  ]
}
